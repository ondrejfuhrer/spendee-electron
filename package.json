{
  "name": "spendee-electron",
  "productName": "Spendee",
  "version": "0.9.5-dev",
  "description": "Simple Electron wrapper for Spendee web app",
  "main": "app/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make --platform=darwin",
    "make-zip": "electron-forge make --platform=darwin --targets @electron-forge/maker-zip",
    "make-dmg": "electron-forge make --platform=darwin --targets @electron-forge/maker-dmg",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "test": "mocha"
  },
  "repository": "https://github.com/ondrejfuhrer/spendee-electron",
  "keywords": [
    "Electron",
    "Spendee",
    "App"
  ],
  "author": "Ondrej FÃ¼hrer",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-dmg": "^6.0.0-beta.52",
    "@electron-forge/maker-zip": "^6.0.0-beta.52",
    "@electron-forge/publisher-github": "^6.0.0-beta.52",
    "electron": "^9.2.1",
    "electron-packager": "^15.0.0",
    "mocha": "^8.1.1"
  },
  "dependencies": {
    "electron-store": "^5.2.0",
    "electron-window-state": "^5.0.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "assets/icons/macos/Spendee.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "platforms": [
            "darwin"
          ]
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "ondrejfuhrer",
              "name": "spendee-electron"
            },
            "prerelease": true
          }
        }
      ]
    }
  }
}
