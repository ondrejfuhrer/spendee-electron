{
  "name": "spendee-electron",
  "productName": "Spendee",
  "version": "0.9.4",
  "description": "Simple Electron wrapper for Spendee web app",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "test": "mocha",
    "package-mac": "electron-packager . Spendee --overwrite --platform=darwin --arch=x64 --icon=assets/icons/macos/Spendee.icns --prune=true --out=release-builds"
  },
  "repository": "https://github.com/ondrejfuhrer/spendee-electron",
  "keywords": [
    "Electron",
    "Spendee",
    "App"
  ],
  "author": "Ondrej FÃ¼hrer",
  "license": "MIT",
  "devDependencies": {
    "electron": "^9.2.1"
  },
  "dependencies": {
    "electron-store": "^5.2.0",
    "electron-window-state": "^5.0.3",
    "mocha": "^8.1.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "linux"
          ]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        }
      ]
    }
  }
}
